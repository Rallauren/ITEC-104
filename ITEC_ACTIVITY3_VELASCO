
using System;
using System.Linq;

namespace Array
{
	public static class Program
	{
		public static void Main()
		{
		  
		Console.WriteLine("Index: 0");
          string[] email = new string[]{"ralphlauren@gmail.com", "ralphlauren123@gmail.com", "ralphlauren.velasco@gmail.com"};
          string[] userName = new string[] {"rlauren", "ralph", "ralphlauren"};
		string[] password = new string[] {"ralph123", "ralph@123","ralphlauren"};
	
		int i = 1;
		Console.WriteLine("Email: " + email[i]);
        Console.WriteLine("User name: " + userName[i]);
        Console.WriteLine("Password: " + password[i]);
			
		}
		
		
	}
}
